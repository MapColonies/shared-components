{
  "name": "@map-colonies/react-components",
  "version": "4.0.2",
  "main": "./dist/@map-colonies/react-components.umd.js",
  "module": "./dist/@map-colonies/react-components.es.js",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "clean": "rimraf dist",
    "transpileTS": "tsc --project ./tsconfig-build.json",
    "build": "yarn run clean && yarn printTSVersion && yarn transpileTS && vite build",
    "printTSVersion": "echo TS $(tsc -v)",
    "postbuild": "copyfiles -u 2 -e \"**/*.ts\" -e \"**/*.tsx\" \"src/components/**/*\" dist",
    "prepack": "yarn build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "copyassets:all": "yarn run copyassets:md-font && yarn run copyassets:cesium",
    "copyassets:cesium": "copyfiles -u 5 \"../../node_modules/cesium/Build/Cesium/**/*\" \"./public\"",
    "copyassets:md-font": "copyfiles -u 5 \"../../node_modules/material-design-icons/iconfont/*\" \"./public/fonts/material-icons\"",
    "storybook": "yarn clean && yarn run copyassets:all && start-storybook -p 9010",
    "build-storybook": "build-storybook && yarn run copyassets:md-font"
  },
  "dependencies": {
    "@date-io/date-fns": "^1.3.13",
    "@here/quantized-mesh-decoder": "^1.2.8",
    "@map-colonies/react-core": "^4.0.1",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/pickers": "^3.2.10",
    "@turf/bbox": "^6.0.1",
    "@turf/bbox-polygon": "^6.0.1",
    "@turf/boolean-point-in-polygon": "^6.5.0",
    "@turf/helpers": "^6.1.4",
    "@turf/rewind": "^5.1.5",
    "cesium": "1.103.0",
    "chonky": "^2.3.2",
    "chonky-icon-fontawesome": "^2.3.2",
    "copy-webpack-plugin": "^6.3.1",
    "copyfiles": "2.3.0",
    "cross-env": "7.0.2",
    "date-fns": "^2.14.0",
    "geojson": "^0.5.0",
    "get-input-selection": "^1.1.4",
    "lodash": "^4.17.20",
    "ol": "^6.4.3",
    "resium": "^1.16.1",
    "rimraf": "3.0.2",
    "textarea-caret": "^3.1.0"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "6.5.16",
    "@storybook/addon-storysource": "6.5.16",
    "@storybook/addons": "6.5.16",
    "@storybook/builder-vite": "0.4.2",
    "@storybook/react": "6.5.16",
    "@storybook/theming": "6.5.16",
    "@types/enzyme": "^3.10.5",
    "@types/geojson": "^7946.0.10",
    "@types/lodash": "^4.14.165",
    "@types/ol": "^6.3.1",
    "@types/textarea-caret": "^3.0.1",
    "enzyme": "^3.11.0",
    "jest-enzyme": "^7.1.2",
    "storybook": "6.5.16",
    "typescript": "4.9.5",
    "vite-plugin-cesium": "1.2.22"
  },
  "files": [
    "dist"
  ]
}
