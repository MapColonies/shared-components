{
  "name": "@map-colonies/react-components",
  "version": "4.10.3",
  "main": "./dist/@map-colonies/react-components.umd.js",
  "module": "./dist/@map-colonies/react-components.es.js",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "clean": "rimraf dist",
    "transpileTS": "tsc --project ./tsconfig-build.json",
    "build": "yarn run clean && yarn printTSVersion && yarn transpileTS && vite build",
    "printTSVersion": "echo TS $(tsc -v)",
    "postbuild": "yarn copyassets:all && copyfiles -u 2 -e \"**/*.ts\" -e \"**/*.tsx\" \"src/components/**/*\" dist",
    "prepack": "yarn build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "copyassets:all": "yarn run copyassets:cesium && yarn run copyassets:components",
    "copyassets:cesium": "copyfiles -u 5 \"../../node_modules/cesium/Build/Cesium/**/*\" \"./public\"",
    "copyassets:components": "copyfiles -u 1 \"./public/assets/**/*\" \"dist\"",
    "storybook": "yarn run copy-storybook_data && start-storybook -p 9010",
    "build-storybook": "rimraf storybook-static && build-storybook",
    "copy-storybook_data": "git clone https://github.com/MapColonies/shared-components-data.git && copyfiles -u 1 \"./shared-components-data/stories-data.json\" \".\" && rimraf shared-components-data"
  },
  "dependencies": {
    "@cmcleese/cesium-navigation": "^4.0.7",
    "@date-io/date-fns": "^1.3.13",
    "@here/quantized-mesh-decoder": "^1.2.8",
    "@map-colonies/react-core": "^4.2.0",
    "@material-ui/core": "^4.11.0",
    "@material-ui/pickers": "^3.2.10",
    "@turf/area": "^6.5.0",
    "@turf/bbox": "^6.0.1",
    "@turf/bbox-polygon": "^6.0.1",
    "@turf/boolean-contains": "^6.5.0",
    "@turf/boolean-point-in-polygon": "^6.5.0",
    "@turf/centroid": "^7.2.0",
    "@turf/helpers": "^6.1.4",
    "@turf/intersect": "^6.1.3",
    "@turf/point-to-polygon-distance": "^7.2.0",
    "@turf/rewind": "^5.1.5",
    "cesium": "1.103.0",
    "chonky": "^2.3.2",
    "chonky-icon-fontawesome": "^2.3.2",
    "copy-webpack-plugin": "^6.3.1",
    "copyfiles": "2.3.0",
    "cross-env": "7.0.2",
    "date-fns": "^2.14.0",
    "geojson": "^0.5.0",
    "get-input-selection": "^1.1.4",
    "lodash": "^4.17.20",
    "ol": "^6.4.3",
    "ol-ext": "^4.0.18",
    "p-map": "6.0.0",
    "react-datepicker": "^4.16.0",
    "react-select": "5.7.3",
    "resium": "1.16.1",
    "rimraf": "3.0.2",
    "textarea-caret": "^3.1.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "6.5.16",
    "@storybook/addon-storysource": "6.5.16",
    "@storybook/addons": "6.5.16",
    "@storybook/builder-vite": "0.4.2",
    "@storybook/react": "6.5.16",
    "@storybook/theming": "6.5.16",
    "@types/enzyme": "^3.10.5",
    "@types/geojson": "^7946.0.10",
    "@types/lodash": "^4.14.165",
    "@types/ol": "^6.3.1",
    "@types/ol-ext": "npm:@siedlerchr/types-ol-ext",
    "@types/textarea-caret": "^3.0.1",
    "enzyme": "^3.11.0",
    "jest-enzyme": "^7.1.2",
    "storybook": "6.5.16",
    "typescript": "4.9.5",
    "vite-plugin-cesium": "1.2.22"
  },
  "files": [
    "dist"
  ]
}
